@model IEnumerable<RegionViewModel>
@inject IRegionService regionService

@{
    ViewData["Title"] = "Region";
    var region = Model.ToList();
    
}
    
    <div class="row align-items-center">
    @for (int i = 0; i < region.Count(); i++)
    {
        <div class="col-md-3" style="margin: 10px 0;">
            <div class="card text-white bg-black mb-3 zoom" id="card-box">
                <a asp-area="Admin" asp-controller="Region" asp-action="DetailsAdmin" asp-route-regionid=@region[i].Id><div class="card-header" id="card-oblasti"><h4 class="rubik-dirt-regular align-items-end">@region[i].Name</h4></div></a>
                <div class="card-body" id="card-body">
                </div>
            </div>
        </div>
    }
    </div>
